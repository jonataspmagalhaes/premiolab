{
  "expo": {
    "name": "PremioLab",
    "slug": "premiolab",
    "version": "4.1.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "scheme": "premiolab",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#070a11"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.premiotrader.app",
      "buildNumber": "9",
      "backgroundColor": "#070a11",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "entitlements": {
        "com.apple.security.application-groups": [
          "group.com.premiotrader.app.data"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#070a11"
      },
      "package": "com.premiotrader.app"
    },
    "plugins": [
      "expo-font",
      "expo-secure-store",
      "expo-asset",
      "expo-screen-orientation"
    ],
    "extra": {
      "eas": {
        "projectId": "73b5eb16-af07-43cd-bc76-159fc4e46da9"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/73b5eb16-af07-43cd-bc76-159fc4e46da9"
    }
  }
}
